{

  "manifest_version": 3,
  "name": "Audible Audio Control",
  "version": "1.3.3",

  "description": "Adds a volume controller to the Audible webplayer.",

  "icons": {
    "48": "icons/volume.png"
  },

  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "include_globs": [
        "https://www.audible.*/webplayer*"
      ],
      "js": ["audio-control.js"],
      "css": ["audio-control.css"]
    }
  ]
}
