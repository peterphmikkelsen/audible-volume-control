{

  "manifest_version": 3,
  "name": "Audible Audio Control",
  "version": "1.3",

  "description": "Adds a volume controller to the Audible webplayer.",

  "icons": {
    "48": "icons/volume.png"
  },

  "content_scripts": [
    {
      "matches": ["https://*.audible.co.uk/webplayer*", "https://*.audible.com/webplayer?*", "https://*.audible.de/webplayer*", "https://*.audible.ca/webplayer*", "https://*.audible.com.au/webplayer*", "https://*.audible.co.jp/webplayer*", "https://*.audible.fr/webplayer*", "https://*.audible.it/webplayer*"],
      "js": ["audio-control.js"],
      "css": ["audio-control.css"]
    }
  ]
}
